space = M-space

# Left

q = noop
w = home
e = up
r = end
t = pageup

a = delete
s = left
d = down
f = right
g = pagedown

z = M-z
x = oneshot(customalt)
c = oneshot(customcontrol)
v = oneshot(custommeta)
b = M-b

# Right

y = f7
u = f8
i = f9
o = f10
p = f11
\ = f12

h = f1
