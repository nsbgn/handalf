# Left (Fn)

tab = A-tab
q = rightmeta
w = home
e = up
r = end
t = sysrq

backspace = backspace
a = esc
s = left
d = down
f = right
g = delete

leftshift = rightshift
z = rightcontrol
x = compose
c = pageup
v = pagedown
b = calc

# Right (Fn)

y = scrolllock
u = f7
i = f8
o = f9
p = f10
enter = enter

h = insert
j = f4
k = f5
l = f6
; = f11
' = M-'

n = pause
m = f1
, = f2
. = f3
/ = f12
rightshift = help
