[shift_tap+FnMeta]
leftshift = capslock

[FnMeta]

#leftshift = overload(shift, capslock)
#rightalt = overload(altgr, numlock)
leftshift = overload(shift, oneshot(shift_tap))
leftcontrol = rightcontrol
leftmeta = cancel

space = layer(MetaFn)

# Left (Fn)

tab = tab
q = sysrq
w = home
e = up
r = end
t = pageup

backspace = backspace
a = esc
s = left
d = down
f = right
g = pagedown

leftshift = layer(shift)
z = delete
x = prog1
c = prog2
v = prog3
b = prog4

# Right (Meta)

y = M-y
u = M-u
i = M-i
o = M-o
p = M-p
\ = M-\

h = M-h
j = M-j
k = M-k
l = M-l
; = M-;
' = M-'

n = M-n
m = M-m
, = M-,
. = M-.
/ = M-/
