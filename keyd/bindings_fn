# Left (Fn)
space = timeout(layer(Meta), 140, oneshot(Meta))

tab = A-tab
q = C-esc
w = home
e = up
r = end
t = sysrq

backspace = backspace
a = esc
s = left
d = down
f = right
g = delete

leftshift = rightshift
z = A-esc
x = compose
c = pageup
v = pagedown
b = calc

# Right (Fn)

y = scrolllock
u = f7
i = f8
o = f9
p = f10

h = insert
j = f4
k = f5
l = f6
; = f11
' = rightcontrol

n = pause
m = f1
, = f2
. = f3
/ = f12
rightshift = rightmeta
