space = space

# Left

q = oneshot(leftalt)
w = home
e = up
r = end
t = pageup

a = oneshot(leftmeta)
s = left
d = down
f = right
g = pagedown

z = oneshot(leftcontrol)
# this is menu
x = insert
c = pause
v = delete
b = enter

# Right

y = f12
u = f7
i = f8
o = f9
p = oneshot(rightalt)
\ = scrolllock

h = f11
j = f4
k = f5
l = f6
; = oneshot(rightmeta)
' = sysrq

n = f10
m = f1
, = f2
. = f3
/ = oneshot(rightcontrol)
