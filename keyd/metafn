[MetaFn:M]
space = M-space

# Left (Meta)

q = M-q
w = M-w
e = M-e
r = M-r
t = M-t

a = overload(control, M-a)
s = M-s
d = M-d
f = M-f
g = M-g

z = overload(control, M-z)
x = M-x
c = M-c
v = M-v
b = M-b

# Right (Fn)

y = f12
u = f7
i = f8
o = f9
p = noop
\ = scrolllock

h = f11
j = f4
k = f5
l = f6
; = timeout(oneshot(Alt), 150, overload(Alt, leftalt))
' = capslock

n = f10
m = f1
, = f2
. = f3
/ = timeout(oneshot(Alt), 150, overload(Alt, rightalt))
