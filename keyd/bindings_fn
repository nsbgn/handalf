# Left (Fn)
space = space

tab = A-tab
q = esc
w = home
e = up
r = end
t = pageup

backspace = backspace
a = noop
s = left
d = down
f = right
g = pagedown

leftshift = timeout(overload(shift, rightshift), 140, layer(shift))
z = noop
x = cut
c = copy
v = paste
b = delete

# Right (Fn)

y = insert
u = f7
i = f8
o = f9
p = f10
[ = rightmeta

h = sysrq
j = f4
k = f5
l = f6
; = f11
' = rightcontrol

n = compose
m = f1
, = f2
. = f3
/ = f12
rightshift = layer(alt)
