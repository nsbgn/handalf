# Left (Fn)

tab = A-tab
q = compose
w = home
e = up
r = end
t = noop

backspace = backspace
a = layer(Meta)
s = left
d = down
f = right
g = delete

leftshift = timeout(overload(shift, prog5), 500, overload(shift, rightshift))
z = noop
x = noop
c = pageup
v = pagedown
b = sysrq

# Right (Fn)

y = scrolllock
u = f7
i = f8
o = f9
p = f10
enter = enter

h = insert
j = f4
k = f5
l = f6
; = f11
' = noop

n = pause
m = f1
, = f2
. = f3
/ = f12
rightshift = help
