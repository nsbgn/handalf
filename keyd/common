[global]

chord_timeout = 50
overload_tap_timeout = 300
oneshot_timeout = 500
disable_modifier_guard = 0

[fn]
space = M-A-space

# Left

q = noop
w = home
e = up
r = end
t = noop

a = pageup
s = left
d = down
f = right
g = delete

z = oneshot(control)
x = pause
c = insert
v = pagedown
b = sysrq

# Right

y = f12
u = f7
i = f8
o = f9
p = noop
\ = scrolllock

h = f11
j = f4
k = f5
l = f6
; = noop
' = backspace

n = f10
m = f1
, = f2
. = f3
/ = capslock
rightshift = 
